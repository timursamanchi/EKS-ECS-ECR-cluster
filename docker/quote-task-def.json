{
  "family": "quote-app",
  "executionRoleArn": "arn:aws:iam::040929397520:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "quote-backend",
      "image": "040929397520.dkr.ecr.eu-west-1.amazonaws.com/quote-backend:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    },
    {
      "name": "quote-frontend",
      "image": "040929397520.dkr.ecr.eu-west-1.amazonaws.com/quote-frontend:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "dependsOn": [
        {
          "containerName": "quote-backend",
          "condition": "START"
        }
      ],
      "essential": true
    }
  ]
}
